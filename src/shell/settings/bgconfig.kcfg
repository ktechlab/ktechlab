<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kcfg SYSTEM "http://www.kde.org/standards/kcfg/1.0/kcfg.dtd">
<kcfg>
  <kcfgfile name="kdeveloprc"/>
  <group name="Background Parser">
    <entry name="enable" key="Enabled" type="Bool">
    <default>true</default>
    </entry>
    <entry name="delay" key="Delay" type="Int">
    <default>500</default>
    </entry>
    <entry name="threads" key="Number of Threads" type="Int">
    <default>1</default>
    </entry>
  </group>
</kcfg>
